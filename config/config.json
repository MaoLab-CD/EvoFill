{
  "data": {
    "out_dir": "data/sim_s2_250901/",
    "train_vcf" : "data/sim_s2_250901/YRI_CEU_chr22.vcf.gz",
    "val_vcf"   : "data/sim_s2_250901/CHB_chr22.vcf.gz",
    "fst_mat"   : "data/sim_s2_250901/fst_matrix.csv",
    "tihp": "True"
  },
  "model": {
    "embed_dim": 64,
    "n_heads": 8,
    "n_layers": 4,
    "dropout": 0.1
  },
  "train": {
    "use_fst": "False",
    "batch_size": 256,
    "dataloader_workers": 32,
    "n_var": 2000,
    "site_overlap": 500,
    "epochs": 100,
    "mask_ratio": 0.5,
    "train4test": 500,
    "val_interval": 1,
    "save_interval": 1,
    "save_dir": "./ckpt",
    "logs_dir": "./logs",
    "early_stop": {
      "patience": 5,
      "min_delta": 0.001
    }
  }
}